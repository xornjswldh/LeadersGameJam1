

function on_message(self, message_id, message, sender)
	if message_id==hash("spawn") then
		local effect=false
		if message.effect then
			effect=true
		end
		factory.create("#character",go.get_position("/party"..tostring(message.num)),nil,{anim=message.anim,hp=message.hp,avoid=message.avoid,effect=effect},4)
	end
end
